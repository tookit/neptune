{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "cloudcreativity/laravel-json-api": "1.0.0-beta.6",
        "fideloper/proxy": "^4.0",
        "jaeger/querylist": "^4.1",
        "kalnoy/nestedset": "^4.3",
        "lanin/laravel-api-debugger": "^3.3",
        "laravel/framework": "5.7.*",
        "laravel/horizon": "^1.3",
        "laravel/passport": "^7.0",
        "laravel/tinker": "^1.0",
        "overtrue/laravel-lang": "^3.0",
        "overtrue/laravel-socialite": "^2.0",
        "rinvex/laravel-categories": "^1.0",
        "spatie/async": "^0.0.4",
        "spatie/data-transfer-object": "^1.6",
        "spatie/laravel-json-api-paginate": "^1.4",
        "spatie/laravel-medialibrary": "^7.5",
        "spatie/laravel-permission": "^2.28",
        "spatie/laravel-query-builder": "^1.12",
        "spatie/laravel-schemaless-attributes": "^1.3",
        "spatie/laravel-server-side-rendering": "^1.0",
        "spatie/laravel-sluggable": "^2.1",
        "spatie/laravel-tags": "^2.1",
        "spatie/laravel-validation-rules": "^1.0",
        "spatie/laravel-valuestore": "^1.2",
        "spatie/typed": "^0.1.1",
        "spatie/url": "^1.3",
        "tymon/jwt-auth": "^1.0.0-rc.3"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "barryvdh/laravel-ide-helper": "^2.5",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
